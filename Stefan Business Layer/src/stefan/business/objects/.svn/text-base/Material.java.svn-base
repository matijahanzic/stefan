package stefan.business.objects;

import java.math.BigDecimal;

/**
 *
 * @author Matija
 */
public class Material {
   
    private Integer idShape;   
    private String name;   
    private BigDecimal euroPerKg;
    private MaterialDetails materialDetails;
    private CalculationForm calculationForm;
   
    public Material()
    {}
    

    public Material(Integer idShape)
    {
        this.idShape=idShape;
    }
    
    
    /**
     * @return the idShape
     */

    public Integer getIdShape() {
        return idShape;
    }

    /**
     * @return the name
     */
    public String getName() {
        return name;
    }

    /**
     * @param name the name to set
     */
    public void setName(String name) {
        this.name = name;
    }
    /**
     * @return the euroPerKg
     */
    public BigDecimal getEuroPerKg() {
        return euroPerKg;
    }

    /**
     * @param euroPerKg the euroPerKg to set
     */
    public void setEuroPerKg(BigDecimal euroPerKg) {
        this.euroPerKg = euroPerKg;
    }

    /**
     * @return the materialDetails
     */
    public MaterialDetails getMaterialDetails() {
        return materialDetails;
    }

    /**
     * @param materialDetails the materialDetails to set
     */
    public void setMaterialDetails(MaterialDetails materialDetails) {
        this.materialDetails = materialDetails;
    }

    /**
     * @return the calculationForm
     */
    public CalculationForm getCalculationForm() {
        return calculationForm;
    }

    /**
     * @param calculationForm the calculationForm to set
     */
    public void setCalculationForm(CalculationForm calculationForm) {
        this.calculationForm = calculationForm;
    }
}
